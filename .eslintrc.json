{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "react-app/jest"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-mixed-spaces-and-tabs": 1,
        "indent": ["error", 2]
    }
}
